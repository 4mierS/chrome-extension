{
	"manifest_version": 3,
	"name": "SafeIt4Later",
	"version": "1.0",
	"description": "That is an extension for saving data from the internet.",
	"icons": {
	  "16": "icons/icon16.jpg"
	},
	"action": {
	  "default_icon": "icons/icon16.jpg",
	  "default_popup": "dist/popup.html"
	},
	"background": {
	  "service_worker": "dist/background.js"
	},
	"permissions": [
	  "storage",
	  "activeTab",
	  "contextMenus"

	],
	"host_permissions": [
	  "<all_urls>"
	],
	"content_scripts": [
	  {
		"matches": ["<all_urls>"],
		"js": ["dist/content.js"]
	  }
	]
  }
  